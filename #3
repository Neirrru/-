print (" Введите колличество точек: ")
a = int(input())
s = []
d = 0
h = []
for i in range (1, a+1):
    f = input("Введите координаты: ")
    f = f.split()
    x = int(f[0])
    y = int(f[1])
    s.append((x,y))
for i in s:
    for c in s:
        d = ((i[0] - c[0])**2 + (i[1] - c[1])**2)**0.5
        h.append(d)
print(max(h), min(h))
